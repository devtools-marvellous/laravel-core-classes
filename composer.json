{
    "name": "devtools-marvellous/laravel-core-classes",
    "description": "Main core classes for apis and kit modules",
    "type": "library",
    "version": "1.1.13",
    "authors": [
        {
            "name": "Yure Nery",
            "email": "yurenery@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "facade/ignition": "^2.17.0",
        "devtools-marvellous/rest-actions": "^6.0",
        "devtools-marvellous/security-request": "^3.0",
        "devtools-marvellous/sextant": "^1.2.9"
    },
    "autoload": {
        "psr-4": {
            "AttractCores\\LaravelCoreClasses\\": "src/"
        },
        "files": [
            "src/Helpers/Url.php",
            "src/Helpers/ClassMap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AttractCores\\LaravelCoreClasses\\CoreControllerServiceProvider"
            ]
        }
    }
}
